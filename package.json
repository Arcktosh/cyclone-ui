{
    "name": "cyclone-ui",
    "version": "0.1.3",
    "private": false,
    "description": "A Responsive React Typescript Tailwind Component library",
    "homepage": "https://github.com/Arcktosh/cyclone-ui#readme",
    "bugs": {
        "url": "https://github.com/Arcktosh/cyclone-ui/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Arcktosh/cyclone-ui.git"
    },
    "license": "ISC",
    "author": {
        "name": "Ben Henning",
        "email": "benhenning10@gmail.com",
        "url": "https://github.com/Arcktosh"
    },
    "main": "dist/csj/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build-lib": "npx rimraf ./dist/ ./build/ && prettier --single-quote --write src && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
        "build": "ladle build",
        "build-storybook": "build-storybook",
        "start": "ladle serve",
        "storybook": "start-storybook -p 6006"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "resolutions": {
        "webpack": "^5"
    },
    "devDependencies": {
        "@babel/core": "^7.18.5",
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-react": "^7.17.12",
        "@babel/preset-typescript": "^7.17.12",
        "@ladle/react": "^2.4.2",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@storybook/addon-docs": "^6.5.12",
        "@storybook/builder-webpack5": "^6.5.12",
        "@storybook/react": "^6.5.9",
        "@types/react": "^18.0.12",
        "autoprefixer": "^10.4.7",
        "babel-loader": "^8.2.5",
        "daisyui": "^2.15.3",
        "postcss": "^8.4.14",
        "qrcode.react": "^3.1.0",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "rollup": "^2.75.6",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-terser": "^7.0.2",
        "tailwindcss": "^3.1.2",
        "typescript": "^4.7.3",
        "vcard-creator": "^0.4.2"
    },
    "peerDependencies": {
        "react": "^18.1.0",
        "react-dom": "^18.1.0"
    }
}
